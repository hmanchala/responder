{
  "rimVersion": "2.0",
  "packageId": "core",
  "resources": [{
    "appVersion": {
      "scope": "public",
      "resourceType": "resource",
      "cleanName": "Application version number",
      "description": "This static resource returns the version number of the deployed API-D instance",
      "path": "version",
      "version": "0.0.1",
      "tags": [
        "Core",
        "Version"
      ],
      "directive": {
        "GET": {
          "workflow": "com.temenos.responder.commands.VersionInformation",
          "responses": {
            "200": {
              "item": "com.temenos.responder.commands.ScaffoldVersion"
            }
          }
        }
      }
    }
  }]
}